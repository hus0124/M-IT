package CallBy;

public class CallByValueReference {

	public static void main(String[] args) {
		
		String menu;
		
		
		menu = "Latte";
		
		
		System.out.println("change 실행 전 ");
		System.out.println(menu);
		
		
		change(menu);
		
		System.out.println("change 실행 후 ");
		System.out.println(menu);
		

	}

	static String change(String menu) {
		menu = "Mocca";
		return menu;
		
	}
}
