package CallBy;

public class CallByValueReference {

	public static void main(String[] args) {
		
		String menu;
		
		
		menu = "Latte";
		
		
		System.out.println("change 角青 傈 ");
		System.out.println(menu);
		
		
		String newmenu = change(menu);
		
		System.out.println("change 角青 饶 ");
		System.out.println(newmenu);
		
		if(menu==newmenu)
			System.out.println("悼老林家");
		else
			System.out.println("促弗林家");

	}

	static String change(String menu) {
		menu = "Mocca";
		return menu;
		
	}
}
